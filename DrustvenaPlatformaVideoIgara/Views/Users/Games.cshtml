@model IEnumerable<DrustvenaPlatformaVideoIgara.Models.Product>

<h1>Owned Games</h1>

<table class="table">
    <thead>
        <tr>
            <th>Product ID</th>
            <th>Product Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductId</td>
                <td>@product.ProductName</td>
                <td>@product.Price</td>
            </tr>
        }
    </tbody>
</table>
